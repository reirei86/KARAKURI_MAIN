{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Arduino: Upload and Debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/your_program.out", // ビルド後の実行ファイル
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "miDebuggerPath": "/path/to/gdb", // gdbのパスを指定
        "miDebuggerServerAddress": "localhost:3333",
        "externalConsole": true
      }
    ]
  }
  